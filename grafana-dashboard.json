{
  "title": "Memex Monitoring Dashboard",
  "tags": ["memex", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Request Latency (Max)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "http_server_requests_seconds_max",
          "legendFormat": "{{uri}} - {{method}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "axisLabel": "Latency (seconds)"
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "title": "RabbitMQ Message Activity",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_published_total",
          "legendFormat": "{{queue}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "axisLabel": "Total Messages Published"
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true
        }
      }
    }
  ]
}